{
  "name": "uploadexample",
  "version": "1.0.0",
  "description": "Aplicação de upload no servidor aws, usando docker e mongodb",
  "main": "index.js",
  "dependencies": {
    "concurrently": "^6.5.1"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "npm start --prefix frontend",
    "server": "npm start --prefix backend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "npm install --prefix frontend",
    "server-install": "npm install --prefix backend",
    "install-dev": "concurrently \"npm run server-install\" \"npm run client-install\""
  },
